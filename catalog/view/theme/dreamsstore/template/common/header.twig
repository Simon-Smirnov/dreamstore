<!DOCTYPE html>
<!--[if IE]><![endif]-->
<!--[if IE 8 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie8"><![endif]-->
<!--[if IE 9 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie9"><![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html
        dir="{{ direction }}" lang="{{ lang }}"> <!--<![endif]-->
<head>
    <meta charset="UTF-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{{ title }}</title>
    <base href="{{ base }}"/>
    {% if description %}
        <meta name="description" content="{{ description }}"/>
    {% endif %}
    {% if keywords %}
        <meta name="keywords" content="{{ keywords }}"/>
    {% endif %}
    {# <script src="catalog/view/javascript/jquery/jquery-2.1.1.min.js" type="text/javascript"></script> #}
    {# <link href="catalog/view/javascript/bootstrap/css/bootstrap.min.css" rel="stylesheet" media="screen" />
		<script src="catalog/view/javascript/bootstrap/js/bootstrap.min.js" type="text/javascript"></script> #}
    {# <link href="catalog/view/javascript/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
		<link href="//fonts.googleapis.com/css?family=Open+Sans:400,400i,300,700" rel="stylesheet" type="text/css" /> #}

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link
            href="https://fonts.googleapis.com/css2?family=Nunito:ital,wght@0,200..1000;1,200..1000&display=swap"
            rel="stylesheet">

    {# <link href="catalog/view/theme/default/stylesheet/stylesheet.css" rel="stylesheet"> #}
    {# <link href="/catalog/view/javascript/swiper-master/swiper-bundle.min.css" rel="stylesheet"> #}
    {# <link href="catalog/view/theme/dreamsstore/stylesheet/style.css" rel="stylesheet"> #}
    {# {% for style in styles %}
			<link href="{{ style.href }}" type="text/css" rel="{{ style.rel }}" media="{{ style.media }}"/>
		{% endfor %}
		{% for script in scripts %}
			<script src="{{ script }}" type="text/javascript"></script>
		{% endfor %} #}
    {# <script src="catalog/view/javascript/common.js" type="text/javascript"></script> #}
    {% for link in links %}
        <link href="{{ link.href }}" rel="{{ link.rel }}"/>
    {% endfor %}
    {% for analytic in analytics %}
        {{ analytic }}
    {% endfor %}

    <link rel="stylesheet" href="{{ assets }}/assets/app.min.css">
</head>
<body class="{{ class }} custom-scroll custom-scroll--y">
<div class="body">
    <header>
        <div class="header-top">
            <div class="container">
                <div class="header-top_inner">
                    <div class="header-top_info">
                        <a class="header-top_info-email" href="mailto:dreamsstore@mail.ru">dreamsstore@mail.ru</a>
                        <div class="header-top_info-mode">
                            <p>Пн-пт: 09:00 - 20:00</p>
                            <p>Сб-Вс: 10:00 - 18:00</p>
                        </div>
                    </div>
                    <div class="header-top_about">
                        <div class="header-top_messengers">
                            <a>
                                <svg class="icon-svg" xmlns="http://www.w3.org/2000/svg">
                                    <use xlink:href="/sprite.svg#icon_vk"></use>
                                </svg>
                            </a>
                            <a>
                                <svg class="icon-svg" xmlns="http://www.w3.org/2000/svg">
                                    <use xlink:href="/sprite.svg#icon_telegram"></use>
                                </svg>
                            </a>
                            <a>
                                <svg class="icon-svg" xmlns="http://www.w3.org/2000/svg">
                                    <use xlink:href="/sprite.svg#icon_whatsapp"></use>
                                </svg>
                            </a>
                            <a>
                                <svg class="icon-svg" xmlns="http://www.w3.org/2000/svg">
                                    <use xlink:href="/sprite.svg#icon_viber"></use>
                                </svg>
                            </a>
                        </div>
                        <div class="header-top_block">
                            <a class="header-top_block-phone" href="tel:+79956686050">+7 995 668 60 50</a>
                            <a class="header-top_block-order" href="">Заказать звонок</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="header-bottom">
            <div class="container">
                <div class="header-bottom_inner">
                    <div class="header-bottom_block">
                        <a href="/" class="header-bottom_block-logo">
                            <img src="{{ logo }}">
                        </a>
                        <div class="header-bottom_block-catalog">
                            <div class="header-bottom_block-catalog_btn">
                                <svg class="burger" xmlns="http://www.w3.org/2000/svg">
                                    <use xlink:href="/sprite.svg#icon_burger"></use>
                                </svg>
                                <svg class="cross" xmlns="http://www.w3.org/2000/svg">
                                    <use xlink:href="/sprite.svg#icon_cross"></use>
                                </svg>
                                <span>Каталог</span>
                            </div>
                            <div class="header-bottom_block-catalog_modal">
                                <div class="header-bottom_block-catalog_bg">
                                    <div class="container">
                                        <div class="header-bottom_block-catalog_modal-inner">
                                            <div class="header-bottom_block-catalog_categories">
                                                {{ menu_main }}
                                            </div>
                                            <div class="header-bottom_block-catalog_about">
                                                <a class="header-bottom_block-catalog_about-link">
                                                    <span class="header-bottom_block-catalog_about-text">Все товары в категории</span>
                                                    <svg class="svg-arrow" xmlns="http://www.w3.org/2000/svg">
                                                        <use xlink:href="/sprite.svg#icon_arrow_right"></use>
                                                    </svg>
                                                </a>
                                                <div class="header-bottom_block-catalog_about-inner"></div>
                                            </div>
                                            <div class="header-bottom_block-catalog_info">
                                                <div class="header-bottom_block-catalog_info-inner">
                                                    <h3>Подарочные карты</h3>
                                                    <p>Когда вы не уверены в том, что именно может понравиться близкому
                                                        человеку или боитесь прогадать с размером, электронная
                                                        подарочная
                                                        карта – лучший сюрприз.</p>
                                                    <img src="image/menu/gift_card.png">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="header-bottom_navigation">
                        <ul class="header-bottom_navigation_list">
                            <li class="header-bottom_navigation_list-item"><a href="/atelier">Ателье</a></li>
                            <li class="header-bottom_navigation_list-item"><a href="/giftcard">В подарок</a></li>
                            <li class="header-bottom_navigation_list-item"><a href="/aboutcompany">О нас</a></li>
                            <li class="header-bottom_navigation_list-item"><a href="/blogs">Блог</a></li>
                            <li class="header-bottom_navigation_list-item"><a href="/buyers">Оплата и доставка</a></li>
                            <li class="header-bottom_navigation_list-item"><a href="/cooperation">Сотрудничество</a>
                            </li>
                            <li class="header-bottom_navigation_list-item"><a href="/contact">Контакты</a></li>
                        </ul>
                    </div>
                    <div class="header-bottom_about">
                        <ul class="header-bottom_about-list">
                            <li data-search-btn>
                                <a>
                                    <svg class="icon-svg-stroke" xmlns="http://www.w3.org/2000/svg">
                                        <use xlink:href="/sprite.svg#icon_search"></use>
                                    </svg>
                                </a>
                                {# {{ search }} #}
                            </li>
                            <li class="dropdown">
                                <a class="header-bottom_about-list_login" {% if logged %}
                                    href="/account" {% else %}data-login-btn{% endif %}>
                                    <svg class="icon-svg-stroke" xmlns="http://www.w3.org/2000/svg">
                                        <use xlink:href="/sprite.svg#icon_login"></use>
                                    </svg>
                                    {% if logged %}
                                        <span>{{ points }}</span>
                                    {% endif %}
                                </a>
                            </li>
                            <li>
                                <a class="header-bottom_about-list_wishlist"
                                   {% if logged %}href="{{ wishlist }}"{% else %}
                                data-login-btn{% endif %} id="wishlist-total"
                                   title="{{ text_wishlist }}">
                                    <svg class="icon-svg-stroke" xmlns="http://www.w3.org/2000/svg">
                                        <use xlink:href="/sprite.svg#icon_wishlist"></use>
                                    </svg>
                                    <span data-wishlist-total>{{ text_wishlist }}</span>
                                </a>
                            </li>
                            <li>{{ cart }}</li>
                        </ul>
                    </div>
                    <div class="header-bottom_mobil">
                        <a class="header-bottom_mobil--phone" href="tel:+79956686050">
                            <img
                                    src="{{ assets }}/assets/images/icons/phone.webp">
                        </a>
                        <a data-mobil-menu-btn>
                            <img class="header-bottom_mobil--burger" data-menu-mobil-btn-open
                                 src="{{ assets }}/assets/images/icons/burger.webp">
                            <svg class="header-bottom_mobil--cross" xmlns="http://www.w3.org/2000/svg"
                                 data-menu-mobil-btn-close>
                                <use xlink:href="/sprite.svg#icon_cross"></use>
                            </svg>
                        </a>
                    </div>
                </div>
            </div>
        </div>
        <div class="header-search" data-header-search></div>
        <div class="modal login-modal" data-modal-type="login-modal" hidden>
            <div class="modal__content">
                <div class="modal__box model-popup">
                    <button class="modal__close modal__close--fixed"></button>
                    <div class="model-popup__content" data-login-content></div>
                </div>
            </div>
        </div>
        <div class="modal login-modal" data-modal-type="share" hidden>
            <div class="modal__content">
                <div class="modal__box model-popup">
                    <button class="modal__close modal__close--fixed"></button>
                    <div class="model-popup__content">
                        <script src="https://yastatic.net/share2/share.js"></script>
                        <div class="ya-share2" data-curtain data-size="l" data-shape="round"
                             data-services="vkontakte,telegram,viber,whatsapp"></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="menuModal" data-menu-mobil>
            <nav class="menuModal-nav">
                <ul>
                    <li class="menuModal-nav__item">
                        <a data-menu-modal-catalog-btn-open>Каталог</a>
                        <div class="menuModal-categories">
                            {{ menu_mobil }}
                        </div>
                    </li>
                    <li class="menuModal-nav__item">
                        <a href="/atelier">Ателье</a>
                    </li>
                    <li class="menuModal-nav__item">
                        <a href="/giftcard">В подарок</a>
                    </li>
                    <li class="menuModal-nav__item">
                        <a href="/aboutcompany">О нас</a>
                    </li>
                    <li class="menuModal-nav__item">
                        <a href="/blogs">Блог</a>
                    </li>
                    <li class="menuModal-nav__item">
                        <a href="/buyers">Оплата и доставка</a>
                    </li>
                    <li class="menuModal-nav__item">
                        <a href="/cooperation">Сотрудничество</a>
                    </li>
                    <li class="menuModal-nav__item">
                        <a href="/contact">Контакты</a>
                    </li>
                </ul>
            </nav>
            <div class="menuModal-footer">
                <a class="menuModal-footer__phone" href="tel:+79956686050">+7 995 668 60 50</a>
                <a class="menuModal-footer__call" href="">Заказать звонок</a>
                <ul class="menuModal-footer__list">
                    <li>Пн-пт: 09:00 – 20:00</li>
                    <li>Сб-Вс: 10:00 – 18:00</li>
                </ul>
                <a class="menuModal-footer__mail" href="mailto:dreamsstore@mail.ru">dreamsstore@mail.ru</a>
                <ul class="menuModal-footer__messages">
                    <li>
                        <a>
                            <svg class="icon-svg" xmlns="http://www.w3.org/2000/svg">
                                <use xlink:href="/sprite.svg#icon_vk"></use>
                            </svg>
                        </a>
                    </li>
                    <li>
                        <a>
                            <svg class="icon-svg" xmlns="http://www.w3.org/2000/svg">
                                <use xlink:href="/sprite.svg#icon_telegram"></use>
                            </svg>
                        </a>
                    </li>
                    <li>
                        <a>
                            <svg class="icon-svg" xmlns="http://www.w3.org/2000/svg">
                                <use xlink:href="/sprite.svg#icon_whatsapp"></use>
                            </svg>
                        </a>
                    </li>
                    <li>
                        <a>
                            <svg class="icon-svg" xmlns="http://www.w3.org/2000/svg">
                                <use xlink:href="/sprite.svg#icon_viber"></use>
                            </svg>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </header>
</body>
</html>
